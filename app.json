{
  "expo": {
    "name": "badminton-app-fe",
    "slug": "badminton-app-fe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "MEDIA_LIBRARY"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "expo": {
      "name": "Badminton",
      "slug": "myapp",
      "platforms": [
        "ios",
        "android"
      ],
      "version": "1.0.0",
      "build": {
        "android": {
          "package": "com.example.badminton"
        }
      }
    }
  }
}